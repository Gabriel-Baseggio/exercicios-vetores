package exerciciosVetores3;

import java.util.Scanner;

public class questao4 {

	public static void main(String[] args) {
		
		String[] nomes;
		int qtd;
		Scanner sc = new Scanner(System.in);
		
		System.out.print("Digite a quantidade de nomes do vetor: ");
		qtd = sc.nextInt();
		
		nomes = new String[qtd];
		
		System.out.println(" ======== MENU ========\n"
				+ "1) Cadastrar nome\n"
				+ "2) Pesquisar nome\n"
				+ "3) Listar todos os nome\n"
				+ "0) Sair do programa\n"
				+ "——————–\n"
				+ "Digite sua escolha:\n");
		
		
		sc.close();

	}

}
